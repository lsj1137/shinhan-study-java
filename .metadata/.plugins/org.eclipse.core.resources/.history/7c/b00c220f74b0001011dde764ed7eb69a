package com.shinhan.day02;

public class Review {
	public static void main(String[] args) {
		q1();
	}

	private static void q1() {
		// 변수
		// 1. 선언 : 변수의 성격(타입)을 정해줘야 함.
		int var1;
		int[] var2 = {10, 20, 30};
		// 2. 초기화
		var1 = 100;
		// 3. 사용
		System.out.println("var1 = "+var1);
		
	}
	
	
}
