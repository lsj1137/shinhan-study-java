package com.shinhan.day01;

public class TypeConverting {

	public static void main(String[] args) {
		f1();
		f2();
		f3();
		f4();
		f5();
	}

	private static void f1() {
		System.out.println("f1");
		int var1 = 100;
		long var2 = var1;
		System.out.println(var2);
//		long var3 = 100;
//		int var4 = var3; 에러
	}
	
	private static void f2() {
		System.out.println("f2");
		int var1 = 2147483647; //Integer.MAX_VALUE+1
//		long var2 = 2147483648; // 자바 정수 리터럴은 int형임
		long var2 = var1 + 1;
		System.out.println(var2);
		long var3 = (long) var1 + 1;
		System.out.println(var3);
		long var4 = 2147483648L;
		System.out.println(var4);
	}
	
	private static void f3() {
		System.out.println("f3");
		char var1 = 'A';
		int var2 = var1 + 1;
		System.out.println(var2);
		char var3 = (char) var2;
		System.out.println(var3);
	}
	
	private static void f4() {
		System.out.println("f4");
//		float var1 = 3.141592f; 실수리터럴은 기본형이 double이므로 f,F를 붙여줘야함
		double var1 = 3.141592;
		int var2 = (int) var1;
		System.out.println(var2); // 소수점 날아감
		int var3 = 100;
		double var4 = var3;
		System.out.println(var4);
	}
	
	private static void f5() {
		
	}
	}

}
